<body>
   
    <div class="container">
        <div class="row">
            <div id="col" class="col-sm-8 col-md-6 col-lg-4 mx-auto text-center">
                <h1>Bienvenido</h1>

                <div id="formulariodata" class="card mt-4 text-center">
                    <div class="card-header text-white bg-dark">
                        <h4>
                            Ingresa en tu cuenta
                        </h4>
                    </div>

                    <div class="card-body bg-light">
                        <form id="formulario" method="POST" action="subscribete">
                            <input id="usuario" class="form-control mb-3" name="usuario" type="email"
                                placeholder="email">
                            <input id="contrasena" class="form-control mb-3" name="contrasena" type="password"
                                placeholder="contrasena">
                            <button id="enviar" class="btn btn-dark btn-lg btn-block" type="submit">Enviar</button>
                        </form>

                    </div>


                </div>


            </div>





        </div>
    </div>

